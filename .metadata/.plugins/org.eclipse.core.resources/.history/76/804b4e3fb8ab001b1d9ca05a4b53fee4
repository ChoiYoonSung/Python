import random
mine = input("가위/바위/보")
com = ""
result = ""

rnd = random.random()
if rnd >= 0.66:
    com = "가위"
elif rnd < 0.66 and rnd > 0.33:
    com = "바위"
else:
    com = "보"

if com == mine:
    result = "비김"
elif com == "가위":
    if mine == "바위":
        result = "이김"
    else:
        result = "짐"
elif com == "바위":
    if mine == "가위":
        result = "이김"
    else:
        result = "짐"
elif com == "보":
    if mine == "가위":
        result = "이김"
    else:
        result = "짐"