class JindoDog:
    def __init__(self):
        self.power_of_bark = 0
    
    def train(self):
        self.power_of_bark += 1
    
class SokchoSeagull:
    def __init__(self):
        self.flag_fly = False
    def practice(self):
        self.flag_fly = True

class DogBird(JindoDog, SokchoSeagull):
    def __init__(self, JindoDog, SokchoSeagull):
        super().JindoDog.__init__()
        super().SokchoSeagull.__init__()
        
print(DogBird().power_of_bark)