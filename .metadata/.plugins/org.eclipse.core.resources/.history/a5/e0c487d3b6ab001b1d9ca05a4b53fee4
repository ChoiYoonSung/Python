import random
from dask.array.random import randint

mine = input("홀/짝을 선택하세요")
com = '' 
com2 = '' 

rnd = randint(1,3)%2
rand = random.random()
if rnd == 1:
    com = '홀'
else:
    com = '짝'
    
if rand < 0.5:
    com2 = "홀"
else:
    com2 = "짝"

if mine == com:
    print("답 : " + com +", 맞췄습니다.")
else:
    print("답 : " + com +", 틀렸습니다.")
