class JindoDog:
    def __init__(self):
        self.power_of_bark = 0
    
    def train(self):
        self.power_of_bark += 1
    
class SokchoSeagull:
    def __init__(self):
        self.flag_fly = False
    def practice(self):
        self.flag_fly = True

class DogBird(JindoDog, SokchoSeagull):
    def __init__(self, JindoDog, SokchoSeagull):
        super().__init__()
        
print(DogBird().power_of_bark)
print(DogBird().flag_fly)
DogBird(JindoDog, SokchoSeagull).train()
DogBird(JindoDog, SokchoSeagull).practice()
print(DogBird(JindoDog, SokchoSeagull).power_of_bark)
print(DogBird(JindoDog, SokchoSeagull).flag_fly)